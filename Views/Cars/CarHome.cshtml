<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Car Dealership</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
  </head>
  <body>
    <div class="container">
      <h1>Welcome to Timo's Car Dealership!</h1>
      <p><a href="/cars/new">Add another car</a></p>
      <ul>
        @foreach(var car in Model)
        {
          <h3>@car.GetMake()</h3>
          <h3>@car.GetCost()</h3>
          <h3>@car.GetYear()</h3>
          <img src=@car.GetPic()>
        }
      </ul>
    </div>
  </body>
</html>
